<?xml version="1.0" encoding="ASCII"?>
<!-- the sun libraries are huge in 6.0.   As a temporary hack to get regression passing on the 6.0 libraries, I'm going to use
     these aggressive exclusions -->
<com.ibm.wala.common:EContainer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.ibm.wala.common="http:///com/ibm/wala/wala.ecore.common" xmlns:com.ibm.wala.regex="http:///com/ibm/wala/wala.ecore.regex">
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="java\/awt\/.*"/>
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="javax\/swing\/.*"/>
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="sun\/awt\/.*"/>
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="sun\/swing\/.*"/>
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="com\/sun\/.*"/>
<contents xsi:type="com.ibm.wala.regex:EPattern" pattern="sun\/.*"/>
</com.ibm.wala.common:EContainer>
